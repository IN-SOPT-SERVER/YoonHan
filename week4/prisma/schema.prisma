generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id       Int      @id @default(autoincrement())
  title    String   @db.VarChar(100)
  is_liked Boolean? @default(false)
  summary  String?  @db.VarChar(400)
}

model User {
  id       Int     @id @default(autoincrement())
  userName String
  age      Int?
  email    String? @db.VarChar(400)
}
